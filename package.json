{
  "name": "blog-pc-v2",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:Yancey-Blog/blog-pc-v2.git",
  "author": "Yancey Leo <yanceyofficial@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "commit": "npx git-cz",
    "push": "git push --follow-tags origin master",
    "release:major": "node_modules/.bin/standard-version --release-as major && yarn push",
    "release:minor": "node_modules/.bin/standard-version --release-as minor && yarn push",
    "release:patch": "node_modules/.bin/standard-version --release-as patch && yarn push",
    "lint": "eslint pages --ext .ts,.tsx"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-batch-http": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "graphql": "^14.6.0",
    "next": "9.2.2",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^13.7.2",
    "@types/react": "^16.9.20",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "typescript": "^3.7.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
